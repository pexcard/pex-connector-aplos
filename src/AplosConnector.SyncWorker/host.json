{
  "version": "2.0",
  "aggregator": {
    "batchSize": 1
  },
  "functionTimeout": "00:10:00",
  "extensions": {
    "queues": {
      "maxDequeueCount": 1,
      "batchSize": 1
    }
  },
  "logging": {
    "logLevel": {
      "default": "Information"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": false
      }
    }
  }
}